{
    "name": "delboy1978uk/passport",
    "description": "Passport Control - an ACL service",
    "license": "MIT",
    "authors": [
        {
            "name": "Derek Stephen McLean",
            "email": "delboy1978uk@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "delboy1978uk/barnacle": "^2.2",
        "delboy1978uk/bone-doctrine": "^2.1"
    },
    "require-dev":{
        "ext-xdebug": "*",
        "roave/security-advisories": "dev-master",
        "delboy1978uk/dev-tools": "^1.0"
    },
    "autoload":{
        "psr-4":{
            "Del\\Passport\\": "src/"
        }
    },
    "autoload-dev":{
        "psr-4":{
            "DelTest\\Passport\\": "tests/unit/Del/Passport/"
        }
    },
    "config": {
        "allow-plugins": {
            "phpro/grumphp": false
        }
    }
}
